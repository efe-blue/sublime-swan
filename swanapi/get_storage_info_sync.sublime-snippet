try {
	const res = swan.getStorageInfoSync()
	${1:console.log(res.keys)  // 当前storage中所有的key
	console.log(res.currentSize)  // 当前占用的空间大小, 单位kb
	console.log(res.limitSize)}  // 限制的空间大小，单位kb
} catch (e) {
	${2:// Do something when catch error}
}
]]></content>
